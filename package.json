{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:rss": "ts-node --project utils/tsconfig.json utils/build-rss.ts",
    "build:sitemap": "next-sitemap --config .next-sitemap.js",
    "build:all": "npm run build && npm run build:rss && npm run build:sitemap",
    "export": "next export",
    "deploy": "npm run build:all && next export && firebase deploy",
    "start": "next start",
    "test": "jasmine-ts --config=jasmine.json --project src/specs/tsconfig.json",
    "lint": "eslint src/**/*.ts src/**/*.tsx utils/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts src/**/*.tsx utils/**/*.ts"
  },
  "dependencies": {
    "firebase": "^8.2.5",
    "next": "^12.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.23",
    "@types/chromedriver": "^81.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/prettier": "^2.1.6",
    "@types/react": "^17.0.0",
    "@types/rss": "0.0.28",
    "@types/selenium-webdriver": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cheerio": "^1.0.0-rc.5",
    "chromedriver": "^88.0.0",
    "command-line-args": "^5.1.1",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "fast-xml-parser": "^3.17.6",
    "jasmine": "^3.6.4",
    "jasmine-ts": "^0.4.0",
    "moment": "^2.29.1",
    "next-sitemap": "^1.5.2",
    "node-fetch": "^2.6.1",
    "parse5": "^6.0.1",
    "prettier": "^2.2.1",
    "rss": "^1.2.2",
    "sass": "^1.32.5",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  }
}
